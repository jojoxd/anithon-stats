{
  "name": "anistats",
  "version": "1.0.0",
  "description": "",
  "scripts": {
    "build": "yarn run tsc",
    "barrels": "barrelsby --config .barrelsby.json",
    "tsc": "tsc --project tsconfig.compile.json",
    "tsc:w": "tsc --project tsconfig.json -w",
    "start": "tsnd --inspect --ignore-watch node_modules --respawn --transpile-only -r tsconfig-paths/register src/index.ts",
    "start:debug": "node --inspect -r ts-node/register -r tsconfig-paths/register src/index.ts",
    "start:prod": "cross-env NODE_ENV=production node dist/index.js",
    "build:packages": "yarn build:package-shared && yarn build:package-tsed-auth && yarn build:package-tsed-entity-mapper && yarn build:package-anilist",
    "typeorm": "tsed typeorm"
  },
  "resolutions": {
    "graphql": "^15.8.0"
  },
  "devDependencies": {
    "@jojoxd/tsconfig": "1.1.3",
    "@tsed/cli-plugin-typeorm": "4.2.3",
    "@types/compression": "^1.7.2",
    "@types/cookie-parser": "^1.4.2",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/express-session": "^1.17.4",
    "@types/fs-extra": "^9.0.13",
    "@types/inquirer": "^8.2.0",
    "@types/jexl": "^2.3.0",
    "@types/method-override": "^0.0.32",
    "@types/multer": "^1.4.7",
    "@types/node": "^17.0.18",
    "@types/skia-canvas": "^0.9.28",
    "enquirer": "^2.3.6",
    "ts-node": "^10.5.0",
    "ts-node-dev": "^1.1.8",
    "tsconfig-paths": "^3.12.0",
    "typescript": "^4.5.5"
  },
  "tsed": {
    "packageManager": "yarn",
    "convention": "default"
  }
}
