{
  "name": "anistats",
  "version": "1.0.0",
  "description": "",
  "scripts": {
    "build": "yarn run tsc",
    "barrels": "barrelsby --config .barrelsby.json",
    "tsc": "tsc --project tsconfig.compile.json",
    "tsc:w": "tsc --project tsconfig.json -w",
    "start": "tsnd --inspect --ignore-watch node_modules --respawn --transpile-only -r tsconfig-paths/register src/index.ts",
    "start:debug": "node --inspect -r ts-node/register -r tsconfig-paths/register src/index.ts",
    "start:prod": "cross-env NODE_ENV=production node dist/index.js",
    "build:packages": "yarn build:package-shared && yarn build:package-tsed-auth && yarn build:package-tsed-entity-mapper && yarn build:package-anilist",
    "create-gql": "graphql-codegen -c graphql-codegen.config.ts",
    "orm": "mikro-orm"
  },
  "resolutions": {
    "graphql": "^15.8.0"
  },
  "mikro-orm": {
    "useTsNode": true,
    "configPaths": [
      "./src/config/mikro-orm/index.ts"
    ]
  },
  "dependencies": {
    "@apollo/client": "^3.7.3",
    "@jojoxd/tsed-auth-expression": "^1.0.0",
    "@jojoxd/tsed-entity-mapper": "^1.0.0",
    "@mikro-orm/core": "^5.6.6",
    "@mikro-orm/knex": "^5.6.6",
    "@mikro-orm/mariadb": "^5.6.6",
    "@mikro-orm/migrations": "^5.6.6",
    "@swc/helpers": "~0.4.11",
    "@tsed/ajv": "7.8.2",
    "@tsed/cli-core": "4.2.3",
    "@tsed/common": "7.8.2",
    "@tsed/core": "7.8.2",
    "@tsed/di": "7.8.2",
    "@tsed/engines": "7.8.2",
    "@tsed/exceptions": "7.8.2",
    "@tsed/json-mapper": "7.8.2",
    "@tsed/mikro-orm": "^7.10.0",
    "@tsed/platform-cache": "7.8.2",
    "@tsed/platform-express": "7.8.2",
    "@tsed/schema": "7.8.2",
    "@tsed/swagger": "^7.7.0",
    "ajv": "^8.10.0",
    "apollo-boost": "^0.4.7",
    "apollo-cache-inmemory": "^1.6.5",
    "apollo-link-http": "^1.5.16",
    "async-mutex": "0.4.0",
    "barrelsby": "^2.5.1",
    "body-parser": "^1.19.2",
    "canvas": "^2.9.0",
    "compression": "^1.7.4",
    "connect-session-knex": "^3.0.1",
    "cookie-parser": "^1.4.6",
    "core-js": "^3.6.5",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "dotenv": "^16.0.0",
    "express": "^4.17.2",
    "express-session": "^1.17.2",
    "graphql": "^15.8.0",
    "graphql-tag": "^2.12.6",
    "include-media": "^1.4.10",
    "jexl": "^2.3.0",
    "luxon": "^3.2.1",
    "mariadb": "^3.0.2",
    "method-override": "^3.0.0",
    "pug": "^3.0.2",
    "regenerator-runtime": "0.13.7",
    "skia-canvas": "^0.9.29",
    "sqlite3": "^5.1.4",
    "tslib": "^2.3.0",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^2.16.3",
    "@graphql-codegen/typescript": "^2.8.7",
