    "@types/cookie-parser": "^1.4.2",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/express-session": "^1.17.4",
    "@types/fs-extra": "^9.0.13",
    "@types/inquirer": "^8.2.0",
    "@types/jexl": "^2.3.0",
    "@types/luxon": "^3.2.0",
    "@types/method-override": "^0.0.32",
    "@types/multer": "^1.4.7",
    "@types/node": "^17.0.18",
    "@types/skia-canvas": "^0.9.28",
    "@types/uuid": "^9.0.0",
    "cross-env": "^7.0.3",
    "enquirer": "^2.3.6",
    "ts-node": "^10.5.0",
    "ts-node-dev": "^1.1.8",
    "tsconfig-paths": "^3.12.0"
