{
  "name": "@anistats/shared",
  "version": "0.0.0",
  "devDependencies": {
    "barrelsby": "^2.5.1",
    "tsup": "^5.12.6"
  },
  "peerDependencies": {
    "luxon": "^3.3.0"
  },
  "scripts": {
    "build:package-shared": "yarn clean && yarn barrels && yarn tsc",
    "clean:package-anilist": "yarn clean",
    "watch:package-shared": "yarn build:package-shared --watch",
    "barrels": "barrelsby -c .barrelsby.json",
    "clean": "rm dist -rf && echo 'Cleaned package-shared'",
    "tsc": "tsup src/index.ts --out-dir dist --format cjs,esm --dts"
  },
  "main": "./src/index.js",
  "typings": "./src/index.d.ts"
}
