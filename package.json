    "packages/shared",
    "frontend"
  ],
  "dependencies": {
    "@anime-rss-filter/anilist": "workspace:packages/anilist",
    "@anistats/shared": "workspace:packages/shared",
    "@tsed/ajv": "^6.59.3",
    "@tsed/cli-core": "3.3.1",
    "@tsed/common": "^6.59.3",
    "@tsed/core": "^6.59.3",
    "@tsed/di": "^6.59.3",
    "@tsed/engines": "^1.1.4",
    "@tsed/exceptions": "^6.59.3",
    "@tsed/json-mapper": "^6.59.3",
    "@tsed/platform-express": "^6.59.3",
    "@tsed/schema": "^6.59.3",
    "@tsed/typeorm": "^6.59.3",
    "ajv": "^8.6.1",
    "async-mutex": "^0.3.2",
    "body-parser": "^1.19.0",
    "canvas": "^2.8.0",
