{
  "name": "@jojoxd/tsed-entity-mapper",
  "version": "0.0.1",
  "description": "Add Entity Mapping to your Route Functions, like PathParams",
  "peerDependencies": {
    "@tsed/common": "^6.114.8",
    "@tsed/core": "^6.114.8",
    "@tsed/di": "^6.114.8",
    "@tsed/platform-params": "^6.114.8",
    "reflect-metadata": "^0.1.13"
  },
  "devDependencies": {
    "@jojoxd/tsconfig": "1.1.3",
    "@swc/core": "^1.2.179",
    "tslint": "^6.1.1",
    "tsup": "^5.12.6",
    "typescript": "^4.3.5"
  },
  "scripts": {
    "build:package-tsed-entity-mapper": "yarn clean && yarn tsc",
    "clean:package-tsed-entity-mapper": "yarn clean",
    "watch:package-tsed-entity-mapper": "yarn build:package-tsed-entity-mapper --watch",
    "clean": "rm dist -rf && echo 'Cleaned package-tsed-entity-mapper'",
    "tsc": "tsup index.ts --out-dir dist --format cjs,esm --dts "
  },
  "main": "dist/index.js"
}
