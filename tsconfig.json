{
  "extends": "@jojoxd/tsconfig/config/base.json",
  "compilerOptions": {
    "baseUrl": ".",
    "outDir": "./dist",
    "noEmit": true,
    "module": "commonjs",
    "typeRoots": [
      "./node_modules/@types"
    ]
  },
  "include": [
    "./src/**/*.ts"
  ],
  "exclude": [
    "node_modules/**",
    "./public",
    "dist",
    "test"
  ],
  "linterOptions": {
    "exclude": []
  }
}
